	<parameter name="estcedula" class="java.lang.String"/>
	<parameter name="cedulaParametro" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    e.estcedula,
    e.estnombre,
    e.estapellido,
    e.estdireccion,
    e.esttelefono,
    e.estgenero,
    c.nombre_curso
FROM estudiantes e
LEFT JOIN matriculas m ON e.estcedula = m.estcedula_per
LEFT JOIN cursos c ON m.id_cur_per = c.id_curso
WHERE e.estcedula = $P{cedulaParametro}
ORDER BY c.nombre_curso;]]>
	</queryString>
