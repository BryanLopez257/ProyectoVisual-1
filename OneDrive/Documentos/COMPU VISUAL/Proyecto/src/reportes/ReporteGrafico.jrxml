	<queryString language="SQL">
		<![CDATA[SELECT
    c.nombre_curso,
    e.estgenero,
    COUNT(e.estcedula) AS total_estudiantes
FROM
    estudiantes e
JOIN
    curso c
    ON e.estcedula = c.estcedula
GROUP BY
    c.nombre_curso,
    e.estgenero
ORDER BY
    c.nombre_curso;]]>
	</queryString>
